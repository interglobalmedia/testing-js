{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "indent": [0],
    "linebreak-style": ["off", "eol-last"],
    "quotes": [0],
    "semi": [0]
  }
}
